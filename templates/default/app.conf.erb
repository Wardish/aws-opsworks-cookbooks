Alias /<%= @document_root %>/ <%= @public_dir %>

DirectoryIndex index.php index.html


<Directory <%= @base_dir %> >
	SetEnv BASE_DIR /<%= @document_root %>

	<% @ci_params.each do |name, value| %>
	SetEnv <%= "#{name}" %> "<%= "#{value}" %>"
	<% end %>

	<% @wp_params.each do |name, value| %>
	SetEnv <%= "#{name}" %> "<%= "#{value}" %>"
	<% end %>


    AllowOverride All
    EnableMMAP Off
    EnableSendfile Off
    Require all granted
</Directory>